<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">


<generatorConfiguration>
    <!--数据库驱动jar -->
    <classPathEntry location="D:\work\apache-maven-3.3.3\repo\mysql\mysql-connector-java\5.1.22\mysql-connector-java-5.1.22.jar" />

    <context id="oracleTables" targetRuntime="MyBatis3">
        <!-- 去除注释 -->
        <commentGenerator>
            <property name="suppressAllComments" value="true" />
        </commentGenerator>

        <!--数据库连接 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://192.168.1.220:3306/saas_sh"
                        userId="check" password="check">
        </jdbcConnection>

        <!--生成实体类 指定包名 以及生成的地址 （可以自定义地址，但是路径不存在不会自动创建  使用Maven生成在target目录下，会自动创建） -->
        <javaModelGenerator targetPackage="com.qmcaifu.parent.dal.model" targetProject="MAVEN">
            <property name="enableSubPackages" value="false" />
            <!--<property name="trimStrings" value="true" />-->
        </javaModelGenerator>

        <!--生成SQLMAP文件 -->
        <sqlMapGenerator targetPackage="com.qmcaifu.parent.dal.dao"  targetProject="MAVEN">
            <property name="enableSubPackages" value="false" />
        </sqlMapGenerator>

        <!--生成Dao文件 可以配置 type="XMLMAPPER"生成xml的dao实现(type="SPRING")  context id="DB2Tables" 修改targetRuntime="MyBatis3"(targetProject="MAVEN")  -->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.qmcaifu.parent.dal.dao"  targetProject="MAVEN">
            <property name="enableSubPackages" value="false" />
        </javaClientGenerator>
        
        <table tableName="base_log" domainObjectName="BaseLog" 
       	 enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" 
                enableSelectByExample="false" selectByExampleQueryId="false" />                
    </context>
</generatorConfiguration>